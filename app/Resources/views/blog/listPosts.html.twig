{% extends 'blog.base.html.twig' %}
{% block posts %}
    {% for post in listObj %}
        <div class="blog-post">
        <h2 class="blog-post-title">{{ post.title }}</h2>
        <p class="blog-post-meta">{{ post.dateTime|date }} by <a href="#">{{ post.author }}</a></p>
            <p>{{ post.tag }}</p>
        <p>{{ post.post }}</p>
        <hr>
        </div>
    {% endfor %}
{% endblock %}